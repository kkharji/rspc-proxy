{
  "name": "@rspc-proxy/react",
  "version": "0.0.3",
  "description": "Experimental rspc proxy for @rspc/react",
  "keywords": [
    "rspc",
    "proxy",
    "react",
    "react-js"
  ],
  "author": "kkharji",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "tsup"
  },
  "dependencies": {
    "@rspc-proxy/client": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "@rspc/client": "0.1.2",
    "@rspc/react": "0.1.2",
    "@tanstack/react-query": "^4.8.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true,
    "sourcemap": true
  },
  "peerDependencies": {
    "@rspc/react": "0.1.2",
    "@rspc/client": "0.1.2",
    "@tanstack/react-query": "4.8.0"
  }
}
