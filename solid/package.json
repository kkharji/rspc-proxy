{
  "name": "@rspc-proxy/solid",
  "version": "0.1.2",
  "description": "Experimental rspc proxy for @rspc/solid",
  "keywords": [],
  "author": "kkharji",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "tsup"
  },
  "dependencies": {
    "@rspc-proxy/client": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^6.2.3",
    "@rspc/solid": "0.1.2",
    "@rspc/client": "0.1.2",
    "@tanstack/solid-query": "^4.8.0",
    "rollup": "^2.79.1",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.5.7",
    "typescript": "^4.8.4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true,
    "sourcemap": true
  },
  "peerDependencies": {
    "@rspc/solid": "0.1.2",
    "@tanstack/solid-query": "^4.6.0"
  }
}
